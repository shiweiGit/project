.flow-process {
   margin: 20rpx 32rpx;

  display: -webkit-flex;
  display: flex;
  justify-content: center;
  align-items: center;
}

.flow-process-item {
  /* flex-grow:1; */
  width: 300rpx;
  height: 210rpx;
  text-align: center;
  padding: 12rpx 0;
  background: #fafafa;
  border-radius: 4px;
  box-shadow: 0 0 1px 1px #e4e4e4;
}

.flow-process-index {
  display: block;
  font-size: 48rpx;
  color: #61a1ff;
}

.flow-process-info{
  display: block;
  font-size: 28rpx;
  color: #7f7f7f;
  text-align: justify;
}

.flow-process-arrow {
  /* flex-grow: 1.5; */
  width: 100rpx;
  height: 34rpx;
  background: url("http://www.gzyueyun.com/m/yueyun-mini-app/assets/img/flow_process_right_arrow.png") no-repeat center/contain;
}
.cooperation-item{
  -webkit-box-sizing:border-box;
  box-sizing:border-box;
  display:-webkit-inline-flex;
  display:inline-flex;
  justify-content: center;
  align-items:center;
  width:25%;
  height:116rpx;
}
.cooperation-item > image {
  width:58rpx;
  height:58rpx;
  margin-right:18rpx;
}

