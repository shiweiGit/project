.service-list {
  background: white;
  border-top: 1px solid #e3e3e3;
  margin-top: 20rpx;
}

.service-item {
  -webkit-box-sizing: border-box;
  box-sizing: border-box;
  display: -webkit-inline-flex;
  display: inline-flex;
  align-items: center;
  width: 50%;
  height: 116rpx;
  padding-left: 68rpx;
  border-bottom: 1px solid #e3e3e3;
}

.service-item:nth-of-type(2n + 1) {
  border-right: 1px solid #e3e3e3;
}

.service-item > image {
  width: 58rpx;
  height: 58rpx;
  margin-right: 18rpx;
}

.service-item > text {
  font-size: 24rpx;
  color: #7f7f7f;
}

